fun main() {
    val name = "Assalamualaikum Wr. Wb"
    val age = 17
    val version = "1.3.70"
    val beli = "ayam goyeng"
    val Respone = "Dah lah"
    print("""
        Nama saya $name
        Saya berusia $age tahun
        Saat ini saya sudah mencapai versi $version
        Kemarin saya beli $beli
        $Respone
    """.trimIndent())
}